alias \
		ls='ls -Nh --color=auto --group-directories-first' \
		ll='ls -Nlh --color=auto --group-directories-first' \
		mkdir='mkdir --parents' \
		du='du -h' \
		grep='grep --color=auto' \
		pacman='pacman --color=auto' \
		yay='yay --color=auto' \
		ht='htop' \
		vi='nvim' \
		svi='sudoedit' \
		s='sudo -v; sudo ' \
		pt='sudo powertop' \
		cl='clear' \
		nf='neofetch' \
		g='git' \
		glg='git log --no-merges' \
		gcp='git cherry-pick' \
		gcps='git cherry-pick --skip' \
		gams='git commit --amend -s' \
		gst='git status' \
		alias grba='git rebase --abort' \
		alias grbc='git rebase --continue' \
		alias grbi='git rebase -i' \
		alias grhh='git reset --hard' \
		alias gp='git push' \
		alias gpf='git push --force' \
		alias ga='git add' \
		alias gaa='git add --all' \
		alias gam='git am' \
		alias grm='git rm' \
		alias glg='git log --stat' \
		alias glog='git log --oneline --decorate --graph' \
		alias grev='git revert' \
		alias gamc='git am --continue' \
		alias gams='git am --skip' \
		alias gama='git am --abort' \
		alias gco='git checkout' \
		alias yaorph='yay -Rns $(yay -Qtdq)' \
		alias yarem='yay -Rns' \
		alias yaupg='yay -Syu' \
		alias yain='yay -S' \
		alias scres='sudo systemctl restart' \
		alias scen='sudo systemctl enable' \
		alias scst='sudo systemctl start' \
		pc='sudo picocom -b 115200 /dev/ttyUSB0' \
		zrel='exec $SHELL' \
		n='nnn -Ruex'
